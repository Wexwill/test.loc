<?php

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function an_task_35_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  $form['phone_field'] = [
    '#type' => 'tel',
    '#title' => 'Phone number',
    '#pattern' => '[^\\d]*',
    '#weight' => -15,
  ];
}
